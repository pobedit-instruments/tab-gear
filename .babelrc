{
	"presets": [
		"es2015",
		"es2016",
		"es2017",
		"react"
	],

	"plugins": [
		"transform-object-rest-spread",
		"transform-decorators-legacy"
	],

	"env": {
		"development": {
			"presets": [
				"react-hmre"
			]
		},

		"production": {
			"plugins": [
				"transform-react-remove-prop-types"
			]
		}
	}
}
